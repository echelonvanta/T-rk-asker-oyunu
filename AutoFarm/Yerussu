local P=game:GetService("Players")
local T=game:GetService("TeleportService")
local S=game:GetService("StarterGui")

local p=P.LocalPlayer
local m=3231515867
local u=6912384795

local function n(m)
 S:SetCore("SendNotification",{Title="AutoFarm",Text=m,Duration=5})
 print("[Notification] "..m)
end

local function t(i)
 n("Teleport ediliyor: "..tostring(i))
 T:Teleport(i,p)
end

local function c()
 n("20 saniye bekleniyor, sonra Botlar klasörü silinecek...")
 wait(20)
 local b=workspace:FindFirstChild("Botlar")
 if not b then n("Botlar klasörü bulunamadı.") return false end
 n("Botlar klasörü siliniyor...")
 b:Destroy()
 n("Botlar klasörü silindi.")
 return true
end

local function f()
 while true do
  n("Alt üsse ulaşıldı. Botlar 20 saniye sonra silinecek.")
  if c() then
   n("Botlar klasörü silindi. 120 saniye bekleniyor.")
   wait(120)
   n("120 saniye geçti. Ana üsse teleport olunuyor.")
   t(m)
   break
  else
   n("Botlar klasörü silinemedi veya bulunamadı.")
   wait(5)
  end
 end
end

while true do
 local i=game.PlaceId
 if i==m then
  n("Ana oyundasınız. Hemen alt üsse teleport olunacak.")
  t(u)
  break
 elseif i==u then
  f()
 else
  n("Şu anda ne ana oyundayız ne de alt üssteyiz. 10 saniye bekleniyor.")
  wait(10)
 end
 wait(1)
end
